{
	"name": "trpc-swr",
	"version": "0.1.5",
	"type": "module",
	"description": "tRPC-ified SWR hooks",
	"repository": {
		"type": "git",
		"url": "https://github.com/sachinraja/trpc-swr.git"
	},
	"homepage": "https://github.com/sachinraja/trpc-swr#readme",
	"bugs": {
		"url": "https://github.com/sachinraja/trpc-swr/issues"
	},
	"author": "Sachin Raja <sachinraja2349@gmail.com>",
	"license": "MIT",
	"main": "dist/index.cjs",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.cjs"
		},
		"./infinite": {
			"import": "./dist/infinite.js",
			"default": "./dist/infinite.cjs"
		}
	},
	"files": [
		"dist",
		"infinite"
	],
	"scripts": {
		"build": "tsup",
		"format": "dprint fmt",
		"lint": "run-p lint:*",
		"lint:format": "dprint check",
		"lint:types": "tsc",
		"lint:js": "eslint .",
		"prepare": "husky install",
		"prepublishOnly": "pnpm build",
		"test": "vitest"
	},
	"peerDependencies": {
		"@trpc/client": ">=9.23.2",
		"@trpc/server": ">=9.23.2",
		"react": "^17 || ^18",
		"swr": "^1 || ^2.0.0"
	},
	"devDependencies": {
		"@sachinraja/eslint-config": "0.1.1",
		"@testing-library/dom": "8.16.0",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.3.0",
		"@testing-library/user-event": "14.3.0",
		"@trpc/client": "9.26.2",
		"@trpc/server": "9.26.2",
		"@types/node": "18.6.3",
		"@types/react": "18.0.15",
		"@vitejs/plugin-react": "2.0.0",
		"dprint": "0.30.3",
		"eslint": "8.20.0",
		"get-port": "6.1.2",
		"husky": "8.0.1",
		"jsdom": "20.0.0",
		"nano-staged": "0.8.0",
		"npm-run-all": "4.1.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"swr": "1.3.0",
		"tsup": "6.2.1",
		"typescript": "4.7.4",
		"undici": "5.8.0",
		"vite": "3.0.4",
		"vitest": "0.20.2",
		"zod": "3.17.10"
	},
	"sideEffects": false,
	"nano-staged": {
		"*.{js,ts,md,json,yaml}": "dprint fmt"
	},
	"tsup": {
		"entry": [
			"src/index.tsx",
			"src/infinite.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"dts": {
			"resolve": true
		},
		"splitting": true,
		"clean": true
	}
}
